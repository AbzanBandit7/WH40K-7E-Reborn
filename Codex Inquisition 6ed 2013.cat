    <entry id="ae1a51f2-2c8b-f89a-3451-bb479e26cebe" name="Dark Excommunication - Warp Charge 1" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="1" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="0">
