    <entry id="fbf123fb-c0d3-babc-5d36-e2dc31626d62" name="Psychic Communion - Warp Charge 1" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="1" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="0">
