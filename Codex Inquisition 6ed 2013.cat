      <modifiers/>
      <rules>
        <rule id="0128d0f2-7966-fe35-8cc3-129cea319fb1" name="Grimoire of True Names" hidden="false" page="0">
          <description>When the bearer of the Grimoire of True Names is fighting a challenge against a model with the Daemon special rule, his opponent suffers a -5 penalty to his Weapon Skill, Initiative and Leadership characteristics (to a minimum of 1).</description>
          <modifiers/>
        </rule>
      </rules>
      <profiles/>
      <links/>
    </entry>
    <entry id="383580c3-1791-e560-8888-5e9ab60047aa" name="Hammerhand - Warp Charge 1" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="1" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="0">
