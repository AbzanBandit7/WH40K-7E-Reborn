        <entry id="a99c6b4d-8c8a-d0a4-0928-bd35fdf08a6a" name="Servo Skulls" points="3.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="3" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="0">
          <entries/>
