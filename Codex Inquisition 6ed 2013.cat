    <entry id="e61ec0a8-d4a1-77c7-eec5-2dd151ce57e9" name="Psychic Barrage - Warp Charge 1" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="1" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="0">
