    <entry id="b8be-8aee-90fa-b7a0" name="Elimination Maniple" points="0.0" categoryId="28b94f51-e66b-4096-aa59-0c9df620a77d" type="unit" minSelections="0" maxSelections="-1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" book="Codex: Cult Mechanicus">
      <entries/>
      <entryGroups/>
      <modifiers/>
      <rules>
        <rule id="16f6-5c4c-208b-bc1d" name="Elimination Volley" hidden="false">
          <description>If any Kastelan Robot causes an unsaved Wound, Glancing Hits or Penetrating Hits to an enemy unit using a weapon with Luminagen special rule, all Kataphron Destroyer units in this formation that choose to shoot at the same target gain +1 BS and Ignore Cover special rule until the end of the phase.</description>
          <modifiers/>
        </rule>
      </rules>
      <profiles/>
